    <div class="modal-footer">
        <input type="reset" value="Clear" class="btn-link" />&nbsp;&nbsp;
        <button class="btn" data-dismiss="modal" aria-hidden="true">Close</button>
        <button class="btn btn-primary">Save changes</button>
    </div>
</div>

<div class="modal hide fade" id="feedbackdialog">
    <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
        <h3>Add Feedback</h3>
    </div>
    <div class="modal-body">
        @Html.Partial("_NewFeedback")
    </div>
    <div class="modal-footer">
        <input class="btn btn-link" type="reset" value="Clear" />
        <button class="btn" data-dismiss="modal" aria-hidden="true">Close</button>
        <button class="btn btn-primary" data-dismiss="modal" aria-hidden="true">Save Changes</button>
    </div>

</div>
<script>
    $(function () {

        $(".datepicker").datepicker();
        $("[title]").tooltip();

        $(".popover-element").popover({
            html: true,
            trigger: "click"
        });

        $(".hover-popover-element").popover({
            html: true,
            trigger: "hover"
        });

        $(".richtext").wysihtml5({
            "font-styles": false, //Font styling, e.g. h1, h2, etc. Default true
            "emphasis": true, //Italics, bold, etc. Default true
            "lists": true, //(Un)ordered lists, e.g. Bullets, Numbers. Default true
            "html": false, //Button which allows you to edit the generated HTML. Default false
            "link": true, //Button to insert a link. Default true
            "color": false //Button to change color of font
        });

        $(".edititem").click(function () {
            $("#editdialog").modal("show");
        });

        $("#btn-newfeedback").click(function () {
            //  show modal to delete tag
            $("#feedbackdialog")
                .draggable()
                .css({ "width": "900px", "margin-left": "-450px" })
                .modal("show");
            $("#feedbackdialog .modal-body").css({ "min-height": "650px" });
        });

        $(".viewcontacts").click(function () {
            $("#contactdialog")
                .css({ "width": "900px", "margin-left": "-450px" })
                .modal("show");

            $("#contactdialog .modal-body").css({ "min-height": "590px" });
        });

        $(".updatesample").click(function () {
            //  show modal to delete tag
            $("#editsampledialog").modal("show");
        });

        $(".releaseitem").click(function () {
            //  show modal to delete tag
            $("#releasedialog").modal("show");
        });

        $("#problem-code-help").click(function () {
            $("#problemcode-dialog").modal("show");
        });

        $(".modal-footer .btn-primary").click(function () {
            toastr.success("Those changes were successfully updated.", "Webtrieve9 Says");
            $(".modal").modal("hide");
        });
    });
</script>
