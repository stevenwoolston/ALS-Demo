        margin-top: -10px;
        padding: 10px 10px 36px 10px;
        background-color: #49afcd;
    }

    .sample-nav a {
        color: #ffffff;
        text-decoration: underline;
    }

    table.samplehistory {
        padding: 10px;
        width: 1700px;
    }

    #sample-summary {
        margin-bottom: 20px;
    }

    thead .samplecell {
        font-weight: bold;
        background-color: #0a517b;
        color: #edf7fd;
    }

    .samplecell {
        padding: 5px;
        text-align: left;
        min-width: 150px;
        border-right: 1px solid #dddddd;
    }

    .samplecell:last-child {
        border-right: none;
    }

    .rowhead {
        min-width: 115px;
        padding-left: 10px;
    }

    tbody .rowhead, .testhead {
        background-color: #0a517b;
        color: #edf7fd;
    }

    tfoot .rowhead {
        background-color: none;
        color: #000000;
    }
</style>

